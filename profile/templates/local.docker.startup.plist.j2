<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <key>Label</key>
    <string>local.docker.startup</string>
    <key>ProgramArguments</key>
    <array>
      <string>/usr/bin/osascript</string>
      <string>-e</string>
      <string>if application "Docker" is not running then if (list disks) contains "{{ docker_startup_mount }}" then tell application "Docker" to activate</string>
    </array>
    <key>RunAtLoad</key>
    <false/>
    <key>StandardErrorPath</key>
    <string>{{ docker_startup_stderr_log }}</string>
    <key>StandardOutPath</key>
    <string>{{ docker_startup_stdout_log }}</string>
    <key>StartOnMount</key>
    <true/>
  </dict>
</plist>